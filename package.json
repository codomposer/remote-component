{
  "name": "@paciolan/remote-component-starter",
  "version": "2.0.0-semantic-versioning-for-webpack5",
  "description": "Remote Component Starter",
  "private": true,
  "browser": "dist/main.js",
  "author": "Icetrust",
  "license": "MIT",
  "scripts": {
    "build": "npm run clean && cross-env NODE_ENV=production webpack --mode production",
    "build:one": "npm run clean && node setComponentName.js",
    "build:dev": "npm run clean && cross-env NODE_ENV=development webpack --mode development",
    "webpack-dev-server": "cross-env NODE_ENV=development webpack serve --config webpack.dev.config.js --port 9000 --open",
    "start": "concurrently -n webpack,webpack-dev-server -c green,cyan \"npm run build:dev -- --watch\" \"npm run webpack-dev-server\"",
    "clean": "rimraf dist",
    "cz": "git-cz",
    "test": "echo no tests",
    "test:changed": "npm run test -- --changedSince HEAD",
    "test:coverage": "npm run test -- --coverage",
    "lint": "eslint .",
    "prepare": "husky"
  },
  "dependencies": {
    "@types/highcharts": "^7.0.0",
    "buffer": "^6.0.3",
    "highcharts": "^11.4.8",
    "highcharts-react-official": "^3.2.1",
    "https-browserify": "^1.0.0",
    "react": "^18.3.1",
    "stream-http": "^3.2.0",
    "url": "^0.11.4",
    "webpack-shell-plugin-next": "^2.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.25.2",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.24.7",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@babel/runtime": "^7.25.0",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.8.0",
    "@paciolan/eslint-config-react": "^1.0.4",
    "@paciolan/remote-component": "^2.13.0",
    "@types/node": "^22.0.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "concurrently": "^8.2.2",
    "core-js": "^3.37.1",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "git-cz": "^4.9.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.1.4",
    "prettier": "^3.3.3",
    "react-dom": "^18.3.1",
    "regenerator-runtime": "^0.14.1",
    "rimraf": "^6.0.1",
    "source-map-loader": "^5.0.0",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.4",
    "webpack": "^5.93.0",
    "webpack-assets-manifest": "^5.2.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint && npm run build && npm run test:changed && git add .",
      "pre-push": "npm run test:coverage"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/git-cz"
    }
  }
}
